<p id="notice"><%= notice %></p>

<h1>Listing Product Add Ons</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>**Onlineviewlabel</th>
      <th>**Printviewlabel</th>
      <th>**Posviewlabel</th>
      <th>**Displayindex</th>
      <th>**Visibleonline</th>
      <th>**Visiblepos</th>
      <th>**User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_add_ons.each do |product_add_on| %>
      <tr>
        <td><%= product_add_on.name %></td>
        <td><%= product_add_on.onlineViewLabel %></td>
        <td><%= product_add_on.printViewLabel %></td>
        <td><%= product_add_on.posViewLabel %></td>
        <td><%= product_add_on.displayIndex %></td>
        <td><%= product_add_on.visibleOnline %></td>
        <td><%= product_add_on.visiblePos %></td>
        <td><%= User.find(product_add_on.user_id).name %></td>
        <td><%= link_to 'Show', product_add_on %></td>
        <td><%= link_to 'Edit', edit_product_add_on_path(product_add_on) %></td>
        <td><%= link_to 'Destroy', product_add_on, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product add on', new_product_add_on_path %>
