<p id="notice"><%= notice %></p>

<h1>Listing Product Category Sizes</h1>

<table>
  <thead>
    <tr>
      <th>Prefixlabelonline</th>
      <th>**Prefixlabelprint</th>
      <th>**Prefixlabelpos</th>
      <th>**Postfixlabelonline</th>
      <th>**Postfixlabelprint</th>
      <th>**Postfixlabelpos</th>
      <th>**Productcategory</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_category_sizes.each do |product_category_size| %>
      <tr>
        <td><%= product_category_size.prefixLabelOnline %></td>
        <td><%= product_category_size.prefixLabelPrint %></td>
        <td><%= product_category_size.prefixLabelPos %></td>
        <td><%= product_category_size.postfixLabelOnline %></td>
        <td><%= product_category_size.postfixLabelPrint %></td>
        <td><%= product_category_size.postfixLabelPos %></td>
        <td><%= ProductCategory.find(product_category_size.product_category_id).name %></td>
        <td><%= link_to 'Show', product_category_size %></td>
        <td><%= link_to 'Edit', edit_product_category_size_path(product_category_size) %></td>
        <td><%= link_to 'Destroy', product_category_size, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

