<p id="notice"><%= notice %></p>

<h1>Listing Product Add On Lists</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Onlineviewlabel</th>
      <th>Posviewlabel</th>
      <th>Sortlistbyindex</th>
      <th>Sortlistbyname</th>
      <th>Multipleselection</th>
      <th>Enabled</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @product_add_on_lists.each do |product_add_on_list| %>
      <tr>
        <td><%= product_add_on_list.name %></td>
        <td><%= product_add_on_list.onlineViewLabel %></td>
        <td><%= product_add_on_list.posViewLabel %></td>
        <td><%= product_add_on_list.sortListByIndex %></td>
        <td><%= product_add_on_list.sortListByName %></td>
        <td><%= product_add_on_list.multipleSelection %></td>
        <td><%= product_add_on_list.enabled %></td>
        <td><%=  User.find_by_id(product_add_on_list.user_id).name %></td>
        <td><%= link_to 'Show', product_add_on_list %></td>
        <td><%= link_to 'Edit', edit_product_add_on_list_path(product_add_on_list) %></td>
        <td><%= link_to 'Destroy', product_add_on_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Product add on list', new_product_add_on_list_path %>
